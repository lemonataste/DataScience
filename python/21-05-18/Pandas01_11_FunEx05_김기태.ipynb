    "#함수의 결과값은 언제나 하나이다.\n",
    "# 튜플값 하나인 (a+b,a*b)로 돌려준다.\n",
    "\n",
    "def add_and_mul(a,b):\n",
    "    return a+b,a*b\n",
    "\n",
    "res = add_and_mul(3,4)\n",
    "print(res)\n",
    "\n",
    "#튜플 값을 2개의 결괏값처럼 받고싶다면,\n",
    "\n",
    "res1,res2=add_and_mul(3,4)\n",
    "print(res1,res2)"
   ]
  }
