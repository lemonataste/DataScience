    "def add_mul(choice,*args):\n",
    "    if choice == 'add':\n",
    "        res= 0\n",
    "        for i in args:\n",
    "            res=res+i\n",
    "    elif choice=='mul':\n",
    "        res=1\n",
    "        for i in args:\n",
    "            res=res*i\n",
    "    return res\n",
    "\n",
    "res1=add_mul('add',2,3)\n",
    "print(res1)\n",
    "res2=add_mul('mul',2,3)\n",
    "print(res2)"
   ]
